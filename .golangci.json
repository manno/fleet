{
  "formatters": {
    "enable": ["goimports"]
  },
  "linters": {
    "default": "none",
    "enable": [
      "dupl",
      "errcheck",
      "funlen",
      "ginkgolinter",
      "gocyclo",
      "gosec",
      "govet",
      "ineffassign",
      "misspell",
      "nakedret",
      "revive",
      "staticcheck",
      "unparam",
      "unused"
    ],
    "settings": {
      "funlen": {
        "lines": 270,
        "statements": 110
      },
      "revive": {
        "rules": [
          {
            "name": "blank-imports",
            "severity": "warning"
          },
          {
            "name": "unexported-return",
            "severity": "warning"
          }
        ]
      }
    }
  },
  "version": "2"
}
